[package]
name = "{{packageName}}"
version = "{{appVersion}}"
description = "{{appDescription}}"
homepage = "{{homepage}}"
repository = "{{repoUrl}}"
keywords = {{json_str keywords}}
categories = {{json_str categories}}
authors = ["{{{author}}}"]
readme = "README.md"
license = "{{license}}"
edition = "2018"

[dependencies]
tokio = { version = "0.2", features = ["full"] }